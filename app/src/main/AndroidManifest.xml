<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exapmle.android.englishlearning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StandardActivity"
            android:label="Wersja Standardowa" />
        <activity
            android:name=".KidsActivity"
            android:label="Wersja dla dzieci" />
        <activity
            android:name=".ChooseDifficultyActivity"
            android:label="Nauka" />
        <activity
            android:name=".DifficultyAActivity"
            android:label="Easy" />
        <activity
            android:name=".DifficultyBActivity"
            android:label="Medium" />
        <activity
            android:name=".DifficultyCActivity"
            android:label="Hard" />
        <activity android:name=".DisplayFinalListActivity"></activity>
    </application>

</manifest>